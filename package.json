{
  "name": "@otl-core/section-registry",
  "version": "1.0.0",
  "type": "module",
  "description": "Section registry and page renderer for OTL CMS",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build"
  },
  "keywords": [
    "sections",
    "registry",
    "react",
    "cms"
  ],
  "author": "OTL Core",
  "license": "PolyForm-Shield-1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/otl-core/section-registry.git"
  },
  "dependencies": {
    "@otl-core/analytics": "^1.0.0",
    "@otl-core/block-registry": "^1.0.0",
    "@otl-core/cms-types": "^1.0.0",
    "@otl-core/cms-utils": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
